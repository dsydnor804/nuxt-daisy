{
  "name": "nuxt-daisy",
  "version": "0.3.0",
  "private": true,
  "description": "Nuxt3 Starter with DaisyUI",
  "engines": {
    "node": ">=18.19.0 <19.0.0",
    "pnpm": ">=8.12.0 <9.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "format": "prettier --cache --write \"**/*.{cjs,js,jsx,ts,tsx,vue}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "check:all": "pnpm format && pnpm lint:fix && pnpm test:cov"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "~3.4.3",
    "vue-router": "latest"
